<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="star.jiuji.qianbao">

    <uses-permission android:name="android.permission.REQUEST_INSTALL_PACKAGES"/>
    <uses-permission android:name="android.permission.CAMERA" />

    <uses-feature android:name="android.hardware.camera" />
    <uses-feature android:name="android.hardware.camera.autofocus" />

    <uses-permission android:name="android.permission.KILL_BACKGROUND_PROCESSES" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission
        android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"
        tools:ignore="ProtectedPermissions" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <!-- 获取当前WiFi接入的状态以及WLAN热点的信息 -->
    <uses-permission android:name="android.permission.CHANGE_WIFI_MULTICAST_STATE" />
    <!-- 改变WiFi多播状态 -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <!--震动权限-->
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.VIBRATE" />


    <application
        android:name=".base.App"
        android:allowBackup="true"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        tools:replace="android:icon, android:theme,android:name">
        <activity android:name=".armour.activity.SplashActivity"
            android:theme="@style/welcomeTheme">

            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>

        <activity android:name=".armour.activity.MainActivity" />
        <activity android:name=".armour.activity.GuideActivity" />
        <activity android:name=".armour.activity.LoginActivity" />
        <activity android:name=".armour.activity.HtmlActivity" />
        <service android:name=".armour.service.DownAPKService"/>
        <service android:name=".armour.update.DownloadService"/>

        <provider
            android:name="android.support.v4.content.FileProvider"
            android:authorities="${applicationId}.provider"
            android:exported="false"
            android:grantUriPermissions="true">
            <meta-data
                android:name="android.support.FILE_PROVIDER_PATHS"
                android:resource="@xml/file_paths" />
        </provider>



        <activity android:name="star.jiuji.qianbao.activity.LoadingActivity" />
        <!--日历-->
        <activity
            android:name="star.jiuji.qianbao.activity.CalendarActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--支出和收入-->
        <activity
            android:name="star.jiuji.qianbao.activity.IncomeAndCostActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <activity
            android:name="star.jiuji.qianbao.MainActivity"
            android:launchMode="singleTask" />

        <!--存钱页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.SaveMoneyActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--存钱计划-->
        <activity
            android:name="star.jiuji.qianbao.activity.PlanSaveMoneyActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--整理消费账单-->
        <activity
            android:name="star.jiuji.qianbao.activity.FixXiaofeiCheckActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--选择发卡行-->
        <activity
            android:name="star.jiuji.qianbao.activity.ChoiceIssuingBankActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--提醒记账页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.RemindActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--新建存钱计划-->
        <activity
            android:name="star.jiuji.qianbao.activity.newSaveMoneyPlanActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--主题皮肤-->
        <activity
            android:name="star.jiuji.qianbao.activity.ChangeSkinActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--用户界面-->
        <activity
            android:name="star.jiuji.qianbao.activity.UserInfoActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--关于界面-->
        <activity
            android:name="star.jiuji.qianbao.activity.AboutMeActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--设置页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.SettingActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--密码锁页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.CodedLockActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--货币页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.HuoBiActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--货币设置页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.HuoBiSettingActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--登陆界面-->
        <activity
            android:name="star.jiuji.qianbao.activity.Login1Activity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--设置九宫格解锁页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.GraphicLockActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />

        <!--登陆的时候弹出九宫格解锁页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.LoginLockActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--展示存钱的页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.ShowSaveMoneyPlanActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--存一笔-->
        <activity
            android:name="star.jiuji.qianbao.activity.SaveAPenActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--存钱平台-->
        <activity
            android:name="star.jiuji.qianbao.activity.SavePlatformActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />

        <!--通用的修改页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.UpdateCommonKeyBoardActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />

        <!--钱包显示页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.PayShowActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--设置账户页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.PaySettingsActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--增加支出或者收入的页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.EditIncomeAndCostActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--添加选择账户类型页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.ChoiceAccountTypeActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--新建选择账户-->
        <activity
            android:name="star.jiuji.qianbao.activity.newAddAccountActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--转账页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.TransferActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--消费或者支出详细页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.PayShowDetailActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--分享页面需要的-->
        <activity
            android:name="com.mob.tools.MobUIShell"
            android:screenOrientation="portrait"
            android:theme="@android:style/Theme.Translucent.NoTitleBar"
            android:windowSoftInputMode="stateHidden|adjustResize">
            <intent-filter>
                <data android:scheme="tencent100371282" />

                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.BROWSABLE" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <!--搜索查询页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.SearchResultActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--选择颜色页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.ChoiceAccountColorActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--增加预算页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.BudgetActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--发表朋友圈页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.AddCommunityActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
        <!--webView页面-->
        <activity
            android:name="star.jiuji.qianbao.activity.WebViewActivity"
            android:launchMode="standard"
            android:screenOrientation="portrait" />
    </application>
</manifest>